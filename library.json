{
    "name": "TLSPSK",
    "version": "1.0",
    "description": "TLS-PSK implementation based on mbedtls, usable for ESP8266",
    "keywords": "tls, mbedtls, psk",
    "Repository": {
        "type": "git",
        "url": "https://github.com/breadsword/WiFiClientPSK.git"
    },
    "license": "LGPL-3.0-or-later",
    "frameworks": "Arduino",
    "platforms": "espressif8266",
    "build": {
        "flags": "-DMBEDTLS_CONFIG_FILE='\"config-psk.h\"'"
    },
    "dependencies": [
        {
            "name": "mbedtls-psk"
        },
        {
            "name": "gsl-lite"
        }
    ]
}